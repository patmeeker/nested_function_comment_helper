{
  "name": "nested-function-comment-helper",
  "displayName": "nested-function-comment-helper",
  "description": "automatically comment/uncomment a function's other bracket when you manually comment the first",
  "version": "0.0.1",
  "publisher": "patmeeker",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",

  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "balanced-match": "^1.0.0",
    "node-balanced": "0.0.14"
  }
}
